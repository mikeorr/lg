server_host = 127.0.0.1
server_port = 389
search_base = lookupName=virtualdomains,cn=postfix,cn=mailstore,ou=server,ou=edv,dc=example,dc=net
timeout     = 30
bind        = yes
bind_dn     = cn=postfix,ou=system,ou=accounts,dc=example,dc=net
bind_pw     = XXXXXXXXXXXXX
version     = 3
start_tls        = no
tls_ca_cert_file = /etc/ldap/openldap/collected_cas.pem
query_filter     = (lookupKey=%s)
result_attribute = lookupValue
